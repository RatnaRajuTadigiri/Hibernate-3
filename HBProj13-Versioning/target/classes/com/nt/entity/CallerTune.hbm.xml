<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
	  <class name="com.nt.entity.CallerTune"  table="CALLERTUNE">
	     <id name="tuneId" column="TUNEID" >
	        <generator class="increment"/>
	     </id>
	     
	     <version name="updationCount" column="UPDATIONCOUNT" />
	     
	     <property name="tuneName" column="TUNENAME"  length="20"/>
	     <property name="movieName" column="MOVIENAME" length="20"/>
	  </class>
	</hibernate-mapping>
